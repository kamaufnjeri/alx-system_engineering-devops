#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
for (( i=0; i < 10; i++ ))
do
	echo "Best School"
done;
#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
k=9
while (( "$k" >= 0 ))
do
	echo "Best School"
	((k--))
done
#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
k=10
until (( "$k" == 0 ))
do
        echo "Best School"
        ((k--))
done

#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
k=1
while (( "$k" <= 10 ))
do
        echo "Best School"
        ((k++))
	if (( "$k" == 10 ))
	then
		echo "Hi"
	fi
done

#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
k=1
while (( "$k" <= 10 ))
do
        if (( "$k" == 4 ))
        then
                echo "bad luck"
	elif (( "$k" == 8 ))
	then
		echo "good luck"
	else
		echo "Best School"
	fi
	((k++))
done
#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
k=1
while (( "$k" <= 20 ))
do
	case $k in
	4)
		echo "$k"
		echo "bad luck from China"
		;;

	9)
		echo "$k"
		echo "bad luck from Japan"
		;;

	17)
		echo "$k"
		echo "bad luck from Italy"
		;;

	*)
		echo "$k"
		;;
	esac
        ((k++))
done

#!/usr/bin/env bash
# This script is displaying "Best School" 10 times
k=0
while (( "$k" <= 12 ))
do
        echo "Hour: $k"
	m=1
	while (( "$m" <= 59 ))
	do
		echo "$m"
		((m++))
	done
	((k++))
done

